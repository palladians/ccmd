{
  "name": "kmnd",
  "version": "0.0.1",
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "comments": {
    "scripts": {
      "build": "Builds the CLI with tsup.",
      "dev": "Starts a dev server with watch to rebuild CLI.",
      "format": "Formats all TypeScript files in repo with Prettier.",
      "cleanup": "Deletes node_modules and dist directiories.",
      "test:failure": "Symulates non existing command to test error output."
    }
  },
  "scripts": {
    "build": "tsup && chmod +x dist/cli.js",
    "dev": "tsup --watch --onSuccess \"chmod +x dist/cli.js\"",
    "format": "prettier . --write",
    "cleanup": "rimraf node_modules dist",
    "test:failure": "failhere"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@inkjs/ui": "^1.0.0",
    "execa": "^7.2.0",
    "fast-fuzzy": "^1.12.0",
    "find-up": "^6.3.0",
    "graceful-fs": "^4.2.11",
    "ink": "^4.3.1",
    "meow": "^12.0.1",
    "react": "^18.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/graceful-fs": "^4.1.6",
    "@types/react": "^18.2.20",
    "chalk": "^5.3.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
